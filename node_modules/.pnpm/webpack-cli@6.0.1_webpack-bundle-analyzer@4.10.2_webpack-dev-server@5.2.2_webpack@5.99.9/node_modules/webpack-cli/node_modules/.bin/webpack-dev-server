#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules/webpack-dev-server/bin/node_modules:/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules/webpack-dev-server/node_modules:/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules:/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules/webpack-dev-server/bin/node_modules:/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules/webpack-dev-server/node_modules:/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules:/Users/lhf/Desktop/repo/react-mvc-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../webpack-dev-server@5.2.2_webpack-cli@6.0.1_webpack@5.99.9/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
